{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "yarn.build": {
    "input": [
      "."
    ],
    "output": ".next"
  },
  "babelMacros": {
    "twin": {
      "preset": "stitches",
      "stitchesConfig": "stitches.config.ts",
      "disableShortCss": false
    }
  },
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf .next .swc public/robots.txt public/sitemap*.xml",
    "build:docs": "next build && yarn postbuild",
    "start": "next start",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.6.4",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/loader": "^2.1.2",
    "@mdx-js/react": "^2.1.2",
    "@next-safe/middleware": "workspace:*",
    "@stitches/react": "^1.2.8",
    "next": "v12.2.4-canary.8",
    "next-sitemap": "^3.1.10",
    "nextra": "beta",
    "nextra-theme-docs": "beta",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.3",
    "@types/node": "17.0.18",
    "@types/ramda": "^0.28.15",
    "@types/react": "^18.0.15",
    "babel-plugin-macros": "^3.1.0",
    "tailwindcss": "^3.1.6",
    "twin.macro": "rc",
    "typescript": "^4.5.5"
  }
}
