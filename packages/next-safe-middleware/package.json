{
  "name": "@next-safe/middleware",
  "version": "0.9.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "yarn.build": {
    "input": [
      "src",
      "rollup.config.js"
    ],
    "output": "dist"
  },
  "scripts": {
    "dev": "NODE_ENV=development rollup -c rollup.config.js --watch",
    "clean": "rm -rf .swc dist package.tgz",
    "build:pkg": "NODE_ENV=production rollup -c rollup.config.js && yarn pack",
    "build:e2e": "yarn build:pkg",
    "build:docs": "yarn build:pkg",
    "test": "jest"
  },
  "peerDependencies": {
    "next": ">=12.2",
    "react": ">=17"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "lockfile": "^1.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.3.3",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^28.1.5",
    "@types/lockfile": "^1",
    "@types/node": "^17.0.18",
    "@types/ramda": "^0.27.64",
    "@types/react": "^18.0.15",
    "jest": "^28.1.3",
    "next": "^12.2.2",
    "next-safe": "^3.2.1",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "rollup": "^2.68.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-swc3": "^0.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "author": "nibtime",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nibtime/next-safe-middleware"
  },
  "bugs": {
    "url": "https://github.com/nibtime/next-safe-middleware/issues"
  },
  "homepage": "https://next-safe-middleware.vercel.app",
  "description": "Strict Content-Security-Policy (CSP) for Next.js with composable middleware",
  "keywords": [
    "nextjs",
    "security",
    "content-security-policy",
    "strict-csp",
    "strict-dynamic",
    "report-uri",
    "report-to",
    "reporting-api",
    "middleware",
    "chain",
    "compose"
  ]
}
